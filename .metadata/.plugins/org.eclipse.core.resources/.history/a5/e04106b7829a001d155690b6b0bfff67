		// TODO Auto-generated method stub
String x ="Automation Testing";
System.out.println(x);

String FN = "Achala";
String SN = "Machnoor";
System.out.println(FN+" "+SN);
System.out.println(FN.toLowerCase());
System.out.println(FN.toUpperCase());
int xi ,y;
xi= 950;
y= 150;

System.out.println(xi==y);  
System.out.println(xi!=y); 
System.out.println(xi<y);  
System.out.println(xi>y);
System.out.println(xi>=y); 
System.out.println(xi<=y);  
	}

}
